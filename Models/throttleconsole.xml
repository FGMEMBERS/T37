<?xml version="1.0"?>

<PropertyList>

 <path>throttleconsole.ac</path>

 <animation>
  <type>rotate</type>
  <object-name>throttle_right</object-name>
  <property>/controls/engines/engine[1]/throttle</property>
  <factor>-110</factor>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.02</y-m>
   <z-m>0.29</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>1.0</y>
   <z>0.0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>throttle_left</object-name>
  <property>/controls/engines/engine[1]/throttle</property>
  <factor>-110</factor>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.29</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>1.0</y>
   <z>0.0</z>
  </axis>
 </animation>

</PropertyList>
